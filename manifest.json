{
    "manifest_version": 3,
    "name": "Eureka!",
    "version": "1.0",
    "description": "Your one click solution to Sci-Hub the articles.",
    "permissions": [
      "activeTab" 
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png" 
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  
  
    
  